{
  "author": "Christian Bromann <christian@saucelabs.com>",
  "license": "MIT",
  "name": "cucumber-boilerplate",
  "version": "2.0.0",
  "description": "Boilerplate project to run WebdriverIO tests with Cucumber",
  "homepage": "https://github.com/webdriverio/cucumber-boilerplate",
  "contributors": [
    "W. van Kuipers <github@pwnd.nl>"
  ],
  "repository": {
    "type": "git",
    "getUrl": "https://github.com/webdriverio/cucumber-boilerplate.git"
  },
  "workspaces": ["webdriverio"],
  "bugs": {
    "getUrl": "https://github.com/webdriverio/cucumber-boilerplate/issues"
  },
  "keywords": [
    "wdio",
    "webdriver",
    "webdriverio",
    "cucumber",
    "test",
    "boilerplate",
    "selenium"
  ],
  "private": true,
  "scripts": {
    "ci": "npm run ci:wdio && npm run ci:playwright",
    "ci:wdio": "npm ci --prefix webdriverio",
    "ci:playwright": "npm ci --prefix playwright",
    "install": "npm run install:wdio && npm run install:playwright",
    "install:wdio": "npm install --prefix webdriverio",
    "install:playwright": "npm install --prefix playwright",
    "test": "npm run test:wdio && npm run test:playwright",
    "test:wdio": "npm run test --prefix webdriverio",
    "test:playwright": "npm run test --prefix playwright",
    "test:lint": "npm run test:lint:wdio && npm run test:lint:playwright",
    "test:lint:wdio": "npm run test:lint --prefix webdriverio",
    "test:lint:playwright": "npm run test:lint --prefix playwright"
  }
}
