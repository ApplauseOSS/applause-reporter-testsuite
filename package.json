{
  "license": "MIT",
  "name": "applause-reporter-testsuite",
  "version": "1.0.0",
  "description": "Boilerplate project to run tests against the Applause Automation services",
  "keywords": [
    "wdio",
    "webdriver",
    "webdriverio",
    "cucumber",
    "test",
    "boilerplate",
    "selenium"
  ],
  "scripts": {
    "ci": "npm run ci:selenium && npm run ci:playwright",
    "ci:playwright": "npm ci --prefix playwright",
    "ci:playwright:mocha": "npm ci:mocha --prefix playwright",
    "ci:selenium": "npm ci --prefix selenium",
    "ci:selenium:cucumber": "npm ci:cucumber --prefix selenium",
    "ci:selenium:jest": "npm ci:jest --prefix selenium",
    "ci:selenium:mocha": "npm ci:mocha --prefix selenium",
    "ci:selenium:wdio": "npm ci:wdio --prefix selenium",
    "install": "npm run install:selenium && npm run install:playwright",
    "install:playwright": "npm install --prefix playwright",
    "install:playwright:mocha": "npm run install:mocha --prefix playwright",
    "install:selenium": "npm install --prefix selenium",
    "install:selenium:cucumber": "npm run install:cucumber --prefix selenium",
    "install:selenium:jest": "npm run install:jest --prefix selenium",
    "install:selenium:mocha": "npm run install:mocha --prefix selenium",
    "install:selenium:wdio": "npm run install:wdio --prefix selenium",
    "test": "npm run test:selenium && npm run test:playwright",
    "test:playwright": "npm test --prefix playwright",
    "test:playwright:reporting": "npm run test:reporting --prefix playwright",
    "test:playwright:mocha": "npm run test:mocha --prefix playwright",
    "test:playwright:mocha:reporting": "npm run test:mocha:reporting --prefix playwright",
    "test:selenium": "npm test --prefix selenium",
    "test:selenium:reporting": "npm run test:reporting --prefix selenium",
    "test:selenium:cucumber": "npm run test:cucumber --prefix selenium",
    "test:selenium:cucumber:reporting": "npm run test:cucumber:reporting --prefix selenium",
    "test:selenium:jest": "npm run test:jest --prefix selenium",
    "test:selenium:jest:reporting": "npm run test:jest:reporting --prefix selenium",
    "test:selenium:mocha": "npm run test:mocha --prefix selenium",
    "test:selenium:mocha:reporting": "npm run test:mocha:reporting --prefix selenium",
    "test:selenium:wdio": "npm run test:wdio --prefix selenium",
    "test:selenium:wdio:reporting": "npm run test:wdio:reporting --prefix selenium",
    "test:selenium:wdio:cucumber": "npm run test:wdio:cucumber --prefix selenium",
    "test:selenium:wdio:cucumber:reporting": "npm run test:wdio:cucumber:reporting --prefix selenium",
    "test:selenium:wdio:mocha": "npm run test:wdio:mocha --prefix selenium",
    "test:selenium:wdio:mocha:reporting": "npm run test:wdio:mocha:reporting --prefix selenium",
    "test:lint": "npm run test:lint:selenium && npm run test:lint:playwright",
    "test:lint:playwright": "npm run test:lint --prefix playwright",
    "test:lint:playwright:mocha": "npm run test:lint:mocha --prefix playwright",
    "test:lint:selenium": "npm run test:lint --prefix selenium",
    "test:lint:selenium:cucumber": "npm run test:lint:cucumber --prefix selenium",
    "test:lint:selenium:jest": "npm run test:lint:jest --prefix selenium",
    "test:lint:selenium:mocha": "npm run test:lint:mocha --prefix selenium",
    "test:lint:selenium:wdio": "npm run test:lint:wdio --prefix selenium"
  }
}
